        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "xcel_energy_last_updated_seconds * 1000",
          "instant": true,
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "none",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
